{
  "manifest_version": 3,
  "name": "NoBet",
  "version": "1.0",
  "description": "AI 기반 도박 사이트 차단 크롬 익스텐션",
  "permissions": ["storage", "tabs", "scripting", "webNavigation", "cookie"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js",
    "type": "module" 
  },
  "action": {
    "default_popup": "index.html",
    "key":"ndelpgkjmmjodpiahlckdimagconkfgk",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/main-icon16.png",
    "48": "icons/main-icon48.png",
    "128": "icons/main-icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["blocked.html"],
      "matches": ["<all_urls>"]
    }
  ]
  }

